{% extends 'base.html.twig' %}


{% block body %}

    <body class="app header-fixed sidebar-fixed aside-menu-fixed sidebar-lg-show">

    <div id="preloader"
         style="display: none; position: fixed; left: 0; top: 0; z-index: 1999; width: 100%; height: 100%; overflow: visible; background:rgba(255,255,255,0.5);"></div>



        {{ include('header.html.twig') }}

        <div class="app-body blur-on-pace">

            {{ render(controller('App\\Controller\\Config\\EntMenuController::getMainMenu')) }}


            <main class="main">

                <div class="container-fluid" style="padding-top: 10px">
                    <div id="ui-view" style="opacity: 1;">
                        <div class="animated fadeIn">
                            {% block page_content %}
                                {{ include('main.html.twig') }}
                            {% endblock %}
                        </div>
                    </div>
                </div>

            </main>


        </div>

        {{ include('Util/flashes.html.twig') }}

        {{ include('Util/confirmationModal.html.twig') }}

        {{ include('footer.html.twig') }}


    </body>


{% endblock %}