{
  "name": "crosiersource/crosier-core",
  "description": "O n√∫cleo do Crosier",
  "type": "project",
  "license": "MIT",
  "authors": [
    {
      "name": "Carlos Eduardo Pauluk",
      "email": "carlospauluk@gmail.com"
    }
  ],
  "require": {
    "php": "7.4.*",
    "ext-bcmath": "*",
    "ext-ctype": "*",
    "ext-curl": "*",
    "ext-iconv": "*",
    "ext-intl": "*",
    "ext-json": "*",
    "ext-simplexml": "*",
    "ext-zip": "*",
    "crosiersource/crosierlib-base": "*@dev",
    "dompdf/dompdf": "^0.8.3",
    "friendsofsymfony/jsrouting-bundle": "^2.2",
    "nelmio/cors-bundle": "^1.5",
    "sensio/framework-extra-bundle": "^5.2",
    "sensiolabs/security-checker": "^5.0",
    "symfony/asset": "^4.0",
    "symfony/console": "^4.0",
    "symfony/dotenv": "^4.0",
    "symfony/expression-language": "^4.0",
    "symfony/flex": "^1.1",
    "symfony/form": "^4.0",
    "symfony/framework-bundle": "^4.0",
    "symfony/mercure-bundle": "^0.1.2",
    "symfony/monolog-bundle": "^3.3",
    "symfony/orm-pack": "*",
    "symfony/process": "^4.0",
    "symfony/property-access": "^4.0",
    "symfony/security-bundle": "^4.0",
    "symfony/security-csrf": "4.3.*",
    "symfony/serializer-pack": "*",
    "symfony/swiftmailer-bundle": "^3.1",
    "symfony/translation": "4.3.*",
    "symfony/twig-bundle": "^4.0",
    "symfony/validator": "^4.0",
    "symfony/web-link": "^4.0",
    "symfony/webpack-encore-bundle": "^1.0",
    "symfony/yaml": "^4.0",
    "symfony/var-dumper": "^4.4",
    "twig/extensions": "^1.5"
  },
  "require-dev": {
    "symfony/debug-pack": "*",
    "symfony/maker-bundle": "^1.0",
    "symfony/phpunit-bridge": "^4.0",
    "symfony/profiler-pack": "*",
    "symfony/test-pack": "^1.0",
    "symfony/web-server-bundle": "^4.0"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "replace": {
    "paragonie/random_compat": "2.*",
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php71": "*",
    "symfony/polyfill-php70": "*",
    "symfony/polyfill-php56": "*"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd",
      "security-checker security:check": "script"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "5.*"
    }
  },
  "repositories": [
    {
      "type": "path",
      "url": "../crosierlib-base"
    }
  ]
}
